    "css-loader": "^0.28.1",
    "eslint": "^3.17.0",
    "eslint-config-devine": "^1.7.0",
    "eslint-loader": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
